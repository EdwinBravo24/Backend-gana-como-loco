{
    "version": 2,
    "name": "backend-noutube",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
          "methods": ["OPTIONS", "POST"],
      "dest": "index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://frontend-gana-como-loco.vercel.app",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
    
